{
  "compilerOptions": {
    "outDir": "dist",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": false,
  },
  "include": ["*.ts", "**/*.ts"],
  "exclude": ["node_modules", "data"]
}